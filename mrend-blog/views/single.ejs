                                                            <span class=""></span>
                                                        </div>
                                                        <!-- end hover -->
                                                    </a>
                                                </div>
                                                <!-- end media -->
                                                <div class="blog-meta">
                                                    <h4>
                                                        <a href="/blog/post/<%= post.id %>" title=" ">
                                                            <%= post.title %>
                                                        </a>
                                                    </h4>
                                                    <small><%= post.topic.name %></small>
                                                    <small><%= moment(post.date).fromNow() %></small>
                                                </div>
                                                <!-- end meta -->
                                            </div>
                                            <!-- end blog-box -->
                                        </div>
                                        <!-- end col -->
                                        <% }) %>
                                </div>
                                <!-- end row -->
                            </div>
                            <!-- end custom-box -->

                            <hr class="invis1">

                            <div class="custombox clearfix">
                                <h4 class="small-title">
                                    <span id="comment-quantity"><%= post.comment.length %></span> comment(s)</h4>
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="comments-list">
                                            <% post.comment.forEach(comment => { %>
                                                <div class="media">
                                                    <a class="media-left">
                                                        <img src="/cloapedia/images/author.jpg" alt="" class="rounded-circle">
                                                    </a>
                                                    <div class="media-body">
                                                        <h4 class="media-heading user_name">
                                                            <%= comment.author.name %><small><%= moment(comment.date).fromNow() %></small></h4>
                                                        <p>
                                                            <%= comment.content %>
                                                        </p>
                                                        <button onclick="showReply('<%= comment.content %>','<%= comment.author.name %>','<%= comment._id %>')" class="btn btn-primary btn-sm">Reply</button>&emsp;
                                                        <a onmouseover="this.style.cursor = 'pointer'" onclick="displayReply('<%= comment._id %>')">
                                                            <span id="reply-comment-quantity-<%= comment._id %>"><%= comment.replyComment.length %></span> answer(s)
                                                        </a>
                                                    </div>
                                                </div>
                                                <div id="reply-comment-list-<%= comment._id %>" style="display: none;">
                                                    <% comment.replyComment.forEach(replyComment => { %>
