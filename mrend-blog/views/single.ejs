                                                            <a class="media-left">
                                                                <img src="/cloapedia/images/author.jpg" width="50%" alt="" class="rounded-circle">
                                                            </a>
                                                            <div class="media-body" style="margin-left: -7%;">
                                                                <h4 class="media-heading user_name">
                                                                    <%= replyComment.author.name %><small><%= moment(replyComment.date).fromNow() %></small></h4>
                                                                <p style="margin-top: -3%;">
                                                                    <%= replyComment.content %>
                                                                </p>
                                                            </div>
                                                        </div>
                                                        <% }) %>
                                                </div>
                                                <% }) %>
                                        </div>
                                    </div>
                                    <!-- end col -->
                                </div>
                                <!-- end row -->
                            </div>
                            <!-- end custom-box -->

                            <hr class="invis1">

                            <div class="custombox clearfix">
                                <h4 class="small-title">Leave a Reply</h4>
                                <p style="display: none;" id="reply-comment-notify">Answering for
                                    <i id="reply-comment-content"></i>
                                    <button id="button-cancel">Cancel</button>
                                </p>
                                <style>
                                    #button-cancel {
                                        border: none;
                                        background-color: white;
                                        color: #3b5998;
                                        font-weight: bold;
                                    }
                                    
                                    #button-cancel:hover {
                                        cursor: pointer;
                                    }
                                </style>
                                <div class="row">
                                    <div class="col-lg-12">
                                        <form class="form-wrapper">
                                            <input type="hidden" name="postId" value="<%= post.id %>">
                                            <input type="hidden" name="commentId">
                                            <input type="text" class="form-control" name="authorName" placeholder="Your name">
                                            <input type="text" class="form-control" name="authorEmail" placeholder="Email address">
                                            <textarea class="form-control" name="commentContent" placeholder="Your comment"></textarea>
                                            <button type="submit" name="send-comment" value="comment" class="btn btn-primary">Submit comment</button>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- end page-wrapper -->

                        <script>
                            $(document).ready(function() {
                                $("button[name='send-comment']").click(function() {
                                    if ($(this).val() == 'comment') insertComment();
                                    else insertReplyComment();
                                    return false;
                                })
                                $("#button-cancel").click(function() {
                                    $("#reply-comment-notify").hide();
                                    $("button[name='send-comment']").val('comment');
                                })
                            });

                            function showReply(content, author, commentId) {
                                if (content.length > 50) {
                                    content = content.slice(0, 50) + '...'
                                }
                                var str = '\"' + content + '\" của <b>' + author + '.</b>';
                                $("#reply-comment-content").html(str);
                                $("#reply-comment-notify").show();
                                $("input[name='commentId']").val(commentId);
                                $("input[name='authorName']").focus();
                                $("button[name='send-comment']").val('reply-comment');
                            }

                            function displayReply(commentId) {
                                var x = 'reply-comment-list-' + commentId;
                                if ($("#" + x).css('display') == 'none') $("#" + x).show();
                                else $("#" + x).hide();
                            }

                            function insertComment() {
                                var comment = {};
                                comment.postId = $("input[name='postId']").val();
                                comment.authorName = $("input[name='authorName']").val();
                                if (!comment.authorName) {
                                    $("input[name='authorName']").attr('placeholder', 'Please input name');
                                    $("input[name='authorName']").focus();
                                    return;
                                }
                                comment.authorEmail = $("input[name='authorEmail']").val();
                                comment.content = $("textarea[name='commentContent']").val();
                                if (!comment.content) {
                                    $("textarea[name='commentContent']").attr('placeholder', 'Please input content');
                                    $("textarea[name='commentContent']").focus();
                                    return;
                                }
                                $.ajax({
                                    url: '/blog/send-comment',
                                    method: 'post',
                                    data: JSON.stringify(comment),
                                    contentType: 'application/json',
                                    success: function(response) {
                                        Swal.close();
                                        if (JSON.parse(response) == "faile") {
                                            Swal.fire({
                                                icon: 'error',
                                                title: 'Oops...',
                                                text: 'Something error!',
                                                footer: 'Please try again.'
                                            })
                                        } else {
                                            var comment = JSON.parse(response);
                                            $(".comments-list").append(
                                                '	<div class="media">																																							    ' +
                                                '        <a class="media-left">                                                                                                                                                     ' +
                                                '            <img src="/cloapedia/images/author.jpg" alt="" class="rounded-circle">                                                                                                 ' +
                                                '        </a>                                                                                                                                                                       ' +
                                                '        <div class="media-body">                                                                                                                                                   ' +
                                                '            <h4 class="media-heading user_name">                                                                                                                                   ' +
                                                '               ' + comment.author.name + '<small>vừa mới</small></h4>                                                                                                              ' +
                                                '            <p>' + comment.content + '</p>                                                                                                                                         ' +
                                                '            <button onclick="showReply(\'' + comment.content + '\',\'' + comment.author.name + '\',\'' + comment._id + '\')" class="btn btn-primary btn-sm">Trả lời </button>&emsp;  ' +
                                                '            <a onmouseover="this.style.cursor = \'pointer\'" onclick="displayReply(\'' + comment._id + '\')">                                                                      ' +
                                                '                ' + comment.replyComment.length + ' câu trả lời                                                                                                                    ' +
                                                '            </a>                                                                                                                                                                   ' +
                                                '		</div>                                                                                                                                                                      ' +
                                                '	</div>                                                                                                                                                                          ' +
                                                '   <div id="reply-comment-list-' + comment._id + '"></div>                                                                                                                         '
                                            );
                                            var quantity = parseInt($("#comment-quantity").html()) + 1;
                                            $("#comment-quantity").html(quantity);
                                            //scroll to last comment
                                            $([document.documentElement, document.body]).animate({
                                                scrollTop: $(".comments-list div:last").offset().top - $(window).height() / 2
                                            }, 800);
                                        }
                                    },
                                    error: function(response) {
                                        Swal.close();
                                        alert('Server error! Please try again')
                                    }
                                });
                            }

                            function insertReplyComment() {
                                var replyComment = {};
                                replyComment.postId = $("input[name='postId']").val();
                                replyComment.commentId = $("input[name='commentId']").val();
                                replyComment.authorName = $("input[name='authorName']").val();
                                if (!replyComment.authorName) {
                                    $("input[name='authorName']").attr('placeholder', 'Please input name');
                                    $("input[name='authorName']").focus();
                                    return;
                                }
                                replyComment.authorEmail = $("input[name='authorEmail']").val();
                                replyComment.content = $("textarea[name='commentContent']").val();
                                if (!replyComment.content) {
                                    $("textarea[name='commentContent']").attr('placeholder', 'Please input content');
                                    $("textarea[name='commentContent']").focus();
                                    return;
                                }
                                $.ajax({
                                    url: '/blog/send-reply-comment',
                                    method: 'post',
                                    data: JSON.stringify(replyComment),
                                    contentType: 'application/json',
                                    success: function(response) {
                                        Swal.close();
                                        if (JSON.parse(response) == "faile") {
                                            Swal.fire({
                                                icon: 'error',
                                                title: 'Oops...',
                                                text: 'Something error!',
                                                footer: 'Please try again.'
                                            })
                                        } else {
                                            var replyComment = JSON.parse(response);
                                            var commentId = $("input[name='commentId']").val();
                                            $("#reply-comment-list-" + commentId).append(
                                                '	<div class="media" style="margin-left: 10%; margin-top: -4%;">     						            ' +
                                                '        <a class="media-left">                                                                         ' +
                                                '            <img src="/cloapedia/images/author.jpg" width="50%" alt="" class="rounded-circle">         ' +
                                                '        </a>                                                                                           ' +
                                                '        <div class="media-body" style="margin-left: -7%;">                                             ' +
                                                '            <h4 class="media-heading user_name">                                                       ' +
                                                '               ' + replyComment.author.name + '<small>vừa mới</small></h4>                             ' +
                                                '            <p  style="margin-top: -3%;">' + replyComment.content + '</p>                              ' +
                                                '		</div>                                                                                          ' +
                                                '	</div>                                                                                              '
                                            );
                                            var quantity = parseInt($("#reply-comment-quantity-" + commentId + "").html()) + 1;
                                            $("#reply-comment-quantity-" + commentId + "").html(quantity);
                                            //scroll to last reply comment
                                            $([document.documentElement, document.body]).animate({
                                                scrollTop: $("#reply-comment-list-" + commentId).offset().top - $(window).height() / 2
                                            }, 800);
                                        }
                                    },
                                    error: function(response) {
                                        Swal.close();
                                        alert('Server error! Please try again')
                                    }
                                });
                            }
                        </script>
                    </div>
                    <!-- end col -->

                    <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12">
                        <div class="sidebar">
                            <div class="widget">
                                <h2 class="widget-title">Search</h2>
                                <form class="form-inline search-form">
